{
    "manifest_version": 3,
    "name": "Walmart Seller Scraper",
    "version": "1.0",
    "description": "Scrapes product data from a Walmart seller's pages and exports to CSV.",
    "permissions": [
      "activeTab",
      "scripting",
      "downloads"
    ],
    "host_permissions": [
      "https://www.walmart.com/global/seller/*"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }