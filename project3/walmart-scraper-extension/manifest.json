{
    "manifest_version": 3,
    "name": "Walmart Seller Scraper",
    "version": "1.0",
    "description": "Scrapes product data from a Walmart seller page.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://127.0.0.1:5000/*"
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }